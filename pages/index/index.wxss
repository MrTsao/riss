/**index.wxss**/
@import "../../common/lib/news.wxss";
@import "../../common/lib/riverinfo.wxss";
.class-desc {
  font-size: 28rpx;
  padding: 20rpx 20rpx 10rpx 50rpx;
  align-self: flex-start;
}

.class-type {
  color: #4682b4;
  margin-left: 20rpx;
  font-size: 32rpx;
}

.fun-list {
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: center;
  color: #808080;
}

.item {
  display: flex;
  flex-direction: column;
  padding: 20rpx 10rpx;
  align-items: center;
}

.item-image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 100rpx;
  border: 1rpx solid #4682b4;
}

.item-image image {
  width: 60rpx;
  height: 60rpx;
  padding: 20rpx 20rpx;
}

.item-txt {
  font-size: 28rpx;
  padding: 10rpx 20rpx;
}

swiper {
  height: 200rpx;
  width: 100%;
  letter-spacing: 5rpx;
}

.swiper-item {
  height: 200rpx;
  width: 100%;
}

.ad-item {
  height: 200rpx;
  margin: 10rpx 10rpx;
  background-image: url("data:image/jpg;base64,");
  background-color: #4169E1;
  background-position: center;
  background-size: auto; 
  border-radius: 10rpx;
  display: flex;
  flex-direction: row;
}

.ad-item .txt {
  flex: 6;
  display: flex;
  flex-direction: column;
  letter-spacing: 5rpx;
}

.ad-item .head {
  padding-left: 50rpx;
  padding-top: 50rpx;
  font-size: 42rpx;
  font-weight: bold;
  color: #eee;
}

.ad-item .sub-head {
  padding-left: 50rpx;
  padding-top: 10rpx;
  font-size: 28rpx;
  color: #ddd;
}

.ad-item .img {
  height: 150rpx;
  width: 150rpx;
  border-radius: 150rpx;
  background-color: #fffff0;
  margin: 25rpx 20rpx;
  box-shadow: 0 0 40rpx 5rpx #fffff0;
}

.ad-item image {
  padding: 16rpx 16rpx;
  height: 120rpx;
  width: 120rpx;
  image-rendering: auto;
}