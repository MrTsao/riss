/* pages/riverinfo/riverrank.wxss */

swiper {
  height: 200rpx;
  width: 100%;
  letter-spacing: 5rpx;
}

.swiper-item {
  height: 200rpx;
  width: 100%;
}

.fiextable {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  width: 100%;
}

.tb-head {
  flex-grow: 0;
  display: flex;
  color: #36f;
  font-style: initial;
}

.tb-body {
  flex-grow: 2;
}

.sview {
  display: inline-block;
  width: 100%;
  white-space: nowrap;
}

.table {
  vertical-align: top;
  border: 0px solid darkgray;
  width: 100%;
  display: inline-block;
  font-size: 30rpx;
}

.table-body{
  display: flex;
  flex-direction: column;
}

.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 100rpx;
  align-items: center;
}

.td {
  width: 40%;
  justify-content: center;
  text-align: center;
}

.bg-w {
  background: snow;
}

.bg-g {
  background: #e6f3f9;
}

.th {
  width: 40%;
  justify-content: center;
  background: #36f;
  color: #fff;
  display: flex;
  height: 100rpx;
  align-items: center;
}

.td-head {
  width: 100%;
  justify-content: center;
  text-align: center;
}
.th-head {
  width: 100%;
  justify-content: center;
  background: #36f;
  color: #fff;
  display: flex;
  height: 100rpx;
  align-items: center;
}